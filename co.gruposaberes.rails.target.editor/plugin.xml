<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated RailsTarget -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="railsTarget.presentation.RailsTargetModelWizardID"
            name="%_UI_RailsTargetModelWizard_label"
            class="railsTarget.presentation.RailsTargetModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RailsTargetModelFile.gif">
         <description>%_UI_RailsTargetModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated RailsTarget -->
      <editor
            id="railsTarget.presentation.RailsTargetEditorID"
            name="%_UI_RailsTargetEditor_label"
            icon="icons/full/obj16/RailsTargetModelFile.gif"
            extensions="railstarget"
            class="railsTarget.presentation.RailsTargetEditor"
            contributorClass="railsTarget.presentation.RailsTargetActionBarContributor">
      </editor>
   </extension>

</plugin>
